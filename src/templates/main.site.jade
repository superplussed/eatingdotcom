#site
  fluid-grid.site-page
    fluid-block
      .info
        .site-name {{site.name}}
        .role {{site.role}}
        .date-range {{site.dateRange}}
      .detail
        ng-include(src="'templates/' + site.nameUnderscore + '/article.html'")
        ng-include(src="'templates/tech_box.html'")
          
    ng-include(src="'templates/' + site.nameUnderscore + '/screenshots.html'")
